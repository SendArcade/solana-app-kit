{
  "expo": {
    "name": "solana-social-kit",
    "slug": "solana-social-kit",
    "scheme": "solanasocialstarter",
    "version": "0.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.bhu1tyagi.solanasocialstarter",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.bhu1tyagi.solanasocialstarter",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photos to let you pick images."
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "We need access to your photos to let you pick images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c02fa8da-01f8-4d39-b47f-dc85d7079c7d"
      }
    }
  }
}
